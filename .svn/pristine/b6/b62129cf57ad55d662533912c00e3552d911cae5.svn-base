import request from '@/utils/request'
import urls from '@/store/urls'

export function queryIncomes(query) {
    return request({
        url: 'income/search',
        method: 'post',
        data: query
    })
}

export function queryIncomeDetails(query) {
    return request({
        url: 'income/search-detail',
        method: 'post',
        data: query,
    })
}